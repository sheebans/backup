<div class="qz-resource-viewer-panel">
  {{#if resource.hasNarration}}
    <div class="panel panel-default narration">
      <div class="panel-body">
        {{#if resource.hasOwner}}
          <span class="avatar">
              <img src="{{collection.avatarUrl}}">
              <span class="user">{{ collection.author }}</span>
          </span>
        {{/if}}
        <span class="message">
          {{{resource.narration}}}
        </span>
      </div>
    </div>
  {{/if}}

  {{#if isNotIframeUrl}}
      <div class="panel panel-default not-iframe">
          <div class="panel-header">
              <p class="title">
                {{t 'qz-viewer.not-iframe-url.header_1'}}<br>
                {{t 'qz-viewer.not-iframe-url.header_2'}}
              </p>
          </div>
          <div class="panel-body">
              {{cards.qz-resource-card resource=resource editEnabled=false isLinkOut=true}}
          </div>
          <div class="panel-footer">
              <p class="title">
                {{t 'qz-viewer.not-iframe-url.footer_1'}}
              </p>
              <p class="description">
                {{t 'qz-viewer.not-iframe-url.footer_2'}}<br>
                {{t 'qz-viewer.not-iframe-url.footer_3'}}<br>
                {{t 'qz-viewer.not-iframe-url.footer_4'}}
              </p>
          </div>
      </div>
  {{else}}
    {{#if resourceComponentSelected}}
      {{component resourceComponentSelected resource=resource calculatedResourceContentHeight=calculatedResourceContentHeight}}
    {{else}}
      Not implemented
    {{/if}}
  {{/if}}
</div>
{{#if sendEvents}}
  <button class="btn btn-primary btn-next" disabled={{isNextDisabled}} {{action 'next'}}><span>{{t "common.next"}}</span></button>
{{/if}}
